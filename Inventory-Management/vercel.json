{
    "version": 2,
    "builds": [
        {
          "src": "./index.js",
          "use": "@vercel/node"
        }
      ],
      "routes": [
        {
            "src":"/Auth/(.*)",
            "dest":"index.js"
        },
        {
            "src":"/Product/(.*)",
            "dest":"index.js"
        }
      ]
  }
  